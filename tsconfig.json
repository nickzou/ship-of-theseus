{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames":true,
    "skipLibCheck":true,
    "target": "ES6",
    "verbatimModuleSyntax":true,
    "allowJs":true,
    "resolveJsonModule":true,
    "moduleDetection":"force",
    "strict":true,
    "noUncheckedIndexedAccess":true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "lib":["es2022","dom"],
  }
}